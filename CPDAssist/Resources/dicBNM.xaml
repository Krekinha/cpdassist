                    <Border Background="#FF333333" MouseLeftButtonDown="titleBar_MouseLeftButtonDown" Padding="0,0,0,0" CornerRadius="2, 2, 0, 0"/>
                    <!--Padding="10,15,10,15"-->
                    <StackPanel Orientation="Horizontal" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Stretch">
                        <Image Stretch="Fill" Margin="0,0,4,0" Source="{TemplateBinding Icon}" MaxWidth="21" MaxHeight="21" />
                        <TextBlock TextTrimming="WordEllipsis" Foreground="#BFFFFFFF" VerticalAlignment="Center" 
							FontWeight="Bold" Text="{TemplateBinding Title}" HorizontalAlignment="Left" IsHitTestVisible="False" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
                        <Button Click="minimizeButton_Click" Width="21" Height="21" Focusable="False" Template="{DynamicResource BotaoMinimizar}" />
                        <Button Click="maximizeButton_Click" Width="21" Height="21" Focusable="False" Template="{DynamicResource BotaoMaximizar}" IsEnabled="False"/>
                        <Button Click="closeButton_Click" Width="21" Height="21" Focusable="False" Margin="0,0,5,0" Template="{DynamicResource BotaoFechar}" />
                    </StackPanel>
                    <!-- The window content. -->
                    <Border Grid.Row="1">
                        <AdornerDecorator>
                            <ContentPresenter />
                        </AdornerDecorator>
                    </Border>
                </Grid>
            </Border>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="BlackLoginTemplate" TargetType="{x:Type Window}">
        <Grid>
            <Border CornerRadius="2, 2, 0, 0" Background="#FF54545C" BorderBrush="#FF111111" BorderThickness="1.5">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <!-- The title bar. -->
                    <Border Background="Gray" MouseLeftButtonDown="titleBar_MouseLeftButtonDown" Padding="10,15,10,15" CornerRadius="2, 2, 0, 0"/>
                    <!--Padding="10,15,10,15"-->
                    <StackPanel Orientation="Horizontal" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Stretch">
                        <Image Stretch="Fill" Margin="5,0,4,0" Source="{TemplateBinding Icon}" MaxWidth="21" MaxHeight="21" />
                        <TextBlock TextTrimming="WordEllipsis" Foreground="#BFFFFFFF" VerticalAlignment="Center" Margin="10,0,0,0" 
							FontWeight="Bold" Text="{TemplateBinding Title}" HorizontalAlignment="Left" IsHitTestVisible="False" TextAlignment="Center" />
                    </StackPanel>
                    <!-- The window content. -->
                    <Border Grid.Row="1">
                        <AdornerDecorator>
                            <ContentPresenter />
                        </AdornerDecorator>
                    </Border>
                </Grid>
            </Border>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="BlackListBoxItemButton" TargetType="{x:Type Button}">
        <Grid >
            <TextBlock x:Name="Content" Text="X" HorizontalAlignment="Center" VerticalAlignment="Center"
				Foreground="{StaticResource TextoEnabled}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="Content" Property="Foreground" Value="Green"/>
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter TargetName="Content" Property="Foreground" Value="Red"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="Content" Property="Foreground" Value="Blue"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="ButtonControlTemplate1" TargetType="{x:Type Button}">
        <Grid Background="{StaticResource copy}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <ColorAnimation To="Transparent" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                            <ColorAnimation To="#FF515151" Storyboard.TargetName="BrBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ColorAnimation To="{Binding BrBrush}" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                            <ColorAnimation To="{Binding BgBrush}" Storyboard.TargetName="BrBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ColorAnimation To="sc#0.35, 0.045186203, 0.610495567, 0.5647115" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <ColorAnimation To="Transparent" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                            <ColorAnimation To="#FF3C3C3C" Storyboard.TargetName="BrBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00" />
                            <ColorAnimation To="#FF515151" Storyboard.TargetName="Chrome" Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Duration="0:0:0.00" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="FocusStates">
                    <VisualState x:Name="Focused"/>
                    <VisualState x:Name="Unfocused"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="Chrome" TextBlock.Foreground="{TemplateBinding Foreground}" BorderThickness="{TemplateBinding BorderThickness}" SnapsToDevicePixels="true" Width="20">
                <Border.BorderBrush>
                    <SolidColorBrush x:Name="BrBrush" Color="sc#0, 0.08228271, 0.08228271, 0.08228272" />
                </Border.BorderBrush>
                <Border.Background>
                    <SolidColorBrush x:Name="BgBrush" Color="sc#0.25, 0.045186203, 0.610495567, 0.5647115" />
                </Border.Background>
                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
					Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" 
					SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
					VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
            </Border>
        </Grid>
    </ControlTemplate>

    <!-- Styles -->

    <Style x:Key="BlackButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal">
                                    <Storyboard>
                                        <ColorAnimation To="Transparent" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                                        <ColorAnimation To="#FF515151" Storyboard.TargetName="BrBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation To="#FF515151" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                                        <ColorAnimation To="#FF515151" Storyboard.TargetName="BrBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ColorAnimation To="#FF515151" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                                        <ColorAnimation To="Transparent" Storyboard.TargetName="BrBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ColorAnimation To="Transparent" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                                        <ColorAnimation To="#FF3C3C3C" Storyboard.TargetName="BrBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00" />
                                        <ColorAnimation To="#FF515151" Storyboard.TargetName="Chrome" Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Duration="0:0:0.00" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused">
                                    <Storyboard>
                                        <ColorAnimation To="Transparent" Storyboard.TargetName="BgBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00"/>
                                        <ColorAnimation To="#FFF78A09" Storyboard.TargetName="BrBrush" Storyboard.TargetProperty="Color" Duration="0:0:0.00" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unfocused"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="Chrome" TextBlock.Foreground="{TemplateBinding Foreground}" BorderThickness="{TemplateBinding BorderThickness}" SnapsToDevicePixels="true">
                            <Border.BorderBrush>
                                <SolidColorBrush x:Name="BrBrush" Color="#FF515151" />
                            </Border.BorderBrush>
                            <Border.Background>
                                <SolidColorBrush x:Name="BgBrush" Color="#FF333333" />
                            </Border.Background>
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
								Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" 
								SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                               />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
